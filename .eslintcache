\n\nexport default guards;\n\n\n/**\n * \n * reducer(prevState,action)\n * prevState 是上次计算的值\n * action是操作类型\n */","/Users/lixiaojia/Desktop/01/Afcat-Node/src/store/reducer/topic.js",["166"],"function topic(topic={\n    data:[] \n  },action) {\n    console.log(action.data );\n    switch (action.type) {\n       case \"TOPIC_LOAD\":\n          return {\n            data:action.data \n          }\n    }\n    return topic;\n  }\n  \n  export default topic;\n","/Users/lixiaojia/Desktop/01/Afcat-Node/src/routers/router.config.js",["167"],"\nimport IndexPage from \"./index/index\";\nimport UndefinedPage from \"./404/index\";\nimport LoginPage from \"./login\";\nimport AboutPage from \"./about/index\";\nimport GetStartPage from \"./getstart\";\nimport CreateArticlePage from \"./article/index\";\nimport TopicPage from \"./topic/index\";\nimport { lazy, Suspense } from \"react\";\nimport UnRead from './unread/unread';\nimport SignUpPage from \"./signup\";\nconst APIPage = lazy(()=>import(\"./api/index\"));\n\n\nconst router_list = [\n  {\n    path: \"/\",\n    exact: true,\n    render(props) {\n      return <IndexPage {...props} />\n    }\n  }, {\n    path: \"/getstart\",\n    exact: true,\n    render(props) {\n      return <GetStartPage {...props} />\n    }\n  }, {\n    path: \"/api\",\n    exact: true,\n    render(props) {\n      return <Suspense fallback={<h1>视图请求中</h1>}>\n          <APIPage {...props} />\n      </Suspense>\n    }\n  }, {\n    path: \"/about\",\n    exact: true,\n    render(props) {\n      return <AboutPage {...props} />\n    }\n  },\n  {\n    path: \"/signup\",\n    exact: true,\n    render(props) {\n      return <SignUpPage {...props} />\n    }\n  },\n  {\n    path: \"/login\",\n    exact: true,\n    render(props) {\n      return <LoginPage {...props} />\n    }\n  }, {\n    path: \"/topic/create\",\n    exact: true,\n    render(props) {\n      return <CreateArticlePage {...props} />\n    }\n  }, {\n    path: \"/topic\",\n    //exact: true,\n    render(props) {\n      return <TopicPage {...props} />\n    }\n  }/* , {\n    path: \"/unread\",\n    exact: true,\n    render(props) {\n      return <UnRead {...props} />\n    }\n  }*/,{\n    path: \"\",\n    render(props) {\n      return <UndefinedPage {...props} />\n    }\n  }\n];\n\n/**\n * , {\n    path: \"/set\",\n    exact: true,\n    render(props) {\n      return <UnRead {...props} />\n    }\n  }, {\n    path: \"/loginout\",\n    exact: true,\n    render(props) {\n      return <UnRead {...props} />\n    }\n  }\n  , {\n    path: \"/registry\",\n    exact: true,\n    render(props) {\n      return <UnRead {...props} />\n    }\n * \n * \n */\n\nconst navs = [\n  {\n    to: \"/\",\n    title: \"首页\"\n  },{\n    to: \"/getstart\",\n    title: \"新手入门\"\n  },{\n    to: \"/api\",\n    title: \"API\"\n  },{\n    to: \"/about\",\n    title: \"关于\"\n  },\n  {\n    to: \"/signup\",\n    title: \"注册\"\n  },\n  {\n    to: \"/login\",\n    title: \"登录\"\n  }/*,{\n    to: \"/registry\",\n    title: \"注册\"\n  },{\n    to: \"/set\",\n    title: \"设置\"\n  },{\n    to: \"/loginout\",\n    title: \"退出\"\n  },{\n    to: \"/unread\",\n    title: \"未读消息\"\n  }*/\n];\n\nexport { router_list,navs };","/Users/lixiaojia/Desktop/01/Afcat-Node/src/component/nav.js",[],"/Users/lixiaojia/Desktop/01/Afcat-Node/src/routers/api/index.js",[],"/Users/lixiaojia/Desktop/01/Afcat-Node/src/routers/unread/unread.js",[],"/Users/lixiaojia/Desktop/01/Afcat-Node/src/routers/article/index.js",["168","169","170","171"],"import { Button, Card } from \"antd\";\nimport { useState } from \"react\";\nimport {Link,useHistory} from \"react-router-dom\";\nfunction CreateArticlePage(props) {\n    const [types,setTypes] =useState(\"\");\n    const [title,setTitle] =useState(\"\");\n    const [content,setContent] =useState(\"\");\n    const {push} = useHistory();\n    return <div className=\"view\">\n        <div className=\"wrap\">\n            <Card\n                title=\"发布话题\"\n            >\n                <span className=\"tab-selector\">选择版块：</span>\n                <select name=\"tab\" id=\"tab-value\" value={types} onChange={(e)=>getSelectedOption(e)}>\n                    <option value=\"\">请选择</option>\n                    <option value=\"share\">分享</option>\n                    <option value=\"ask\">问答</option>\n                    <option value=\"job\">招聘</option>\n                    <option value=\"dev\">客户端测试</option>\n                </select>\n                <span id=\"topic_create_warn\"></span>\n                \n                <div>\n                    <textarea id=\"article_title\" value={title}  placeholder=\"标题字数10字以上\" \n                    onChange={(e)=>{setTitle(e.target.value)}}></textarea>\n                </div>\n                <div>\n                    <textarea id=\"article_content\" value={content} placeholder=\"内容输入\" \n                    onChange={(e)=>{setContent(e.target.value)}}></textarea>\n                </div>\n                \n                <Button type=\"primary\" onClick={()=>{\n                    insertArticle();\n                }}>提交</Button>\n            </Card>\n        </div>\n    </div>\n    function getSelectedOption(e){\n        let types=e.target.value;\n        console.log(types);\n        let wranSpan = document.querySelector(\"#topic_create_warn\");\n        if(types=='ask'){\n            wranSpan.innerHTML = \"<strong>提问时，请遵循《提问的智慧》中提及的要点，以便您更接收到高质量回复。</strong>\";\n        }else if(types=='job'){\n            wranSpan.innerHTML = \"<strong>为避免被管理员删帖，发帖时请好好阅读《招聘帖规范》。</strong>\";\n        }else{\n            wranSpan.innerHTML = \"\";\n        }\n        setTypes(e.target.value);\n    }\n    function insertArticle(){\n        types==\"\"?\n            alert(\"必须选择一个版块！\")\n        :\n        title.length>=10?\n            fetch('https://cnodejs.org/api/v1/topics',{\n                method:'post',\n                headers:{\n                    'Accept':'application/json,text/plain,*/*',/* 格式限制：json、文本、其他格式 */\n                    'Content-Type':'application/x-www-form-urlencoded'/* 请求内容类型 */\n                },\n                body:`accesstoken=f7b513a6-fdce-472b-a23f-4ecd5a4fa230&tab=${types}&title=${title}&content=${content}`\n            }).then((response)=>{\n                //由于找不到真实请求response对应的id，发帖成功后直接返回到?tab=dev\n                console.log(response);\n                response.status===200 ?\n                push(`/?tab=${types}`)\n                :\n                console.log(\"发帖失败\");\n            })\n            :alert(\"标题字数少于10个字\");\n        \n    }\n}\n\nexport default CreateArticlePage;","/Users/lixiaojia/Desktop/01/Afcat-Node/src/routers/topic/index.js",["172","173"],"import { Card,Divider  } from \"antd\";\nimport { useEffect } from \"react\";\nimport { useSelector } from \"react-redux\";\nimport { useLoadTopic } from \"../../store/action/topic\";\nimport  TopicComment  from \"./topicComment\";\nimport  AddTopicComment  from \"./addTopicComment\";\nimport \"../../static/css/topic.css\"; \nimport { useLocation } from \"react-router-dom\";\n\nfunction TopicPage(props) {\n    //const { id } = props;\n    const id = useLocation().pathname.split('/')[2];\n   \n    const {data} = useSelector(state=>state.topic);\n    \n    const getData = useLoadTopic();\n    useEffect(() => {\n        getData(id);\n    }, [id])\n    //console.log(data)\n    const content_inner = data.content ;\n    //const author = data.author;\n   \n    return <div className=\"view\">\n        <div className=\"wrap\">\n             <div className=\"topic_top\">\n                 <div className=\"topic_title\">\n                     <h1>{data.title}</h1>\n                 </div>\n                 <div className=\"topic_list\">\n                     <ul>\n                         <li>发布于 {data.create_at}</li>\n                         <li>作者{/*data['author']['loginname']*/}</li>\n                         <li>{data.visit_count} 次浏览</li>\n                         <li>来自{data.tab}</li>\n                     </ul>\n                 </div>\n\n             </div>\n             <Divider />\n             <div className=\"topic_content\" \n                dangerouslySetInnerHTML={{\n                __html: content_inner\n                }}\n             >\n                \n             </div>\n             <TopicComment \n            value = {data.replies}\n        />\n        <AddTopicComment />\n        </div>\n\n        \n\n        \n    </div>\n\n\n    \n}\nexport default TopicPage;\n\n\n\n\n","/Users/lixiaojia/Desktop/01/Afcat-Node/src/routers/about/index.js",[],"/Users/lixiaojia/Desktop/01/Afcat-Node/src/routers/404/index.js",[],"/Users/lixiaojia/Desktop/01/Afcat-Node/src/routers/index/index.js",[],"/Users/lixiaojia/Desktop/01/Afcat-Node/src/routers/signup/index.js",[],"/Users/lixiaojia/Desktop/01/Afcat-Node/src/routers/api/inner.js",[],"/Users/lixiaojia/Desktop/01/Afcat-Node/src/routers/getstart/index.js",[],"/Users/lixiaojia/Desktop/01/Afcat-Node/src/routers/login/index.js",["174","175"],"import { Form, Input } from \"antd\";\nimport { UserOutlined, LockOutlined } from \"@ant-design/icons\";\nimport { Button } from \"antd/lib/radio\";\nimport { useEffect } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { useHistory } from \"react-router-dom\";\nimport \"../../static/css/login.css\";\n\nfunction LoginPage() {\n  const dispatch = useDispatch();\n  const { isLogin, prevPath } = useSelector(state => state.guards);\n  const { replace } = useHistory();\n  useEffect(() => {\n    if (isLogin) {\n      replace(prevPath ? prevPath : \"/\");\n    }\n  }, [isLogin])\n\n  const onFinish = (values) => {\n    dispatch({\n      type: \"GUARDS_LOGIN\"\n    })\n  }\n  return <div className=\"wrap-login\">\n    <Form\n      name=\"normal_login\"\n      className=\"login-form\"\n      initialValues={{ remember: true }}\n      onFinish={onFinish}\n    >\n      <Form.Item\n        name=\"username\"\n        rules={[\n          {\n           