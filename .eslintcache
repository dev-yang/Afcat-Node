) {\n  const id = useLocation().pathname.split('/')[2];\n  const getReplys = useGetReplys();\n  const { data } = useSelector(state=>state.replies);\n  useEffect(() => {\n    getReplys(id);\n  }, [id]) \n  \n  if (!data.replies) {\n     return <div></div>;\n       \n  } \n  \n  const replies = data.replies;\n  if(replies.length == 0){\n     return <div></div>;\n  }\n  \n   \n  let count = data.count;\n  const  getMoreComment=(count) => {\n      getReplys(id,1,count);\n   }\n\n  return ( <Card\n        type=\"inner\"\n        title=\"回复\"\n      >\n\n        {data.replies.map((item, index) => {\n          return <Comment\n              key ={index}\n              author={<a className=\"authorName\">{item.username}</a>}\n              avatar={\n                <Avatar\n                  src={item.avatar}\n                  alt={item.avatar}\n                />\n              }\n              content={  item.content  }\n              datetime={\n                <Tooltip title={moment( item.createdAt).format('YYYY-MM-DD HH:mm:ss')}>\n                  <span>{moment(item.createdAt ).fromNow()}</span>\n                </Tooltip>\n              }\n          />\n           \n        })}\n        {count > 5?<button className=\"getMoreComment\" onClick={() => { getMoreComment(count); }}>查看更多>>></button>:\"\" } \n      </Card>\n     \n  );\n}\nexport default TopicComment;","/Users/lixiaojia/Desktop/01/Afcat-Node/src/store/action/topic.js",["266","267"],"import { useEffect } from \"react\";\nimport { useDispatch } from \"react-redux\";\nimport { http,signHttp } from \"./config\";\nfunction useLoadTopic() { \n  const dispatch = useDispatch();\n    return async (id)=>{ \n    const {data} = await signHttp.get(`/article/${id}`);\n    dispatch({\n      type: \"TOPIC_LOAD\",\n      data: data.results\n    })\n  }\n}\n \nexport {useLoadTopic};","/Users/lixiaojia/Desktop/01/Afcat-Node/src/store/action/config.js",[],"/Users/lixiaojia/Desktop/01/Afcat-Node/src/store/action/topics.js",["26